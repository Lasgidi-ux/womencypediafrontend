{
    "kind": "collectionType",
    "collectionName": "education_modules",
    "info": {
        "singularName": "education-module",
        "pluralName": "education-modules",
        "displayName": "Education Module",
        "description": "Educational modules and lessons about remarkable women"
    },
    "options": {
        "draftAndPublish": true,
        "i18n": true
    },
    "pluginOptions": {
        "i18n": {
            "localized": true
        }
    },
    "attributes": {
        "title": {
            "type": "string",
            "required": true,
            "pluginOptions": {
                "i18n": {
                    "localized": true
                }
            }
        },
        "slug": {
            "type": "uid",
            "targetField": "title"
        },
        "description": {
            "type": "text",
            "pluginOptions": {
                "i18n": {
                    "localized": true
                }
            }
        },
        "order": {
            "type": "integer",
            "default": 1,
            "pluginOptions": {
                "i18n": {
                    "localized": false
                }
            }
        },
        "content": {
            "type": "richtext",
            "pluginOptions": {
                "i18n": {
                    "localized": true
                }
            }
        },
        "coverImage": {
            "type": "media",
            "multiple": false,
            "allowedTypes": [
                "images"
            ],
            "pluginOptions": {
                "i18n": {
                    "localized": false
                }
            }
        },
        "lessons": {
            "type": "json",
            "pluginOptions": {
                "i18n": {
                    "localized": true
                }
            }
        },
        "quiz": {
            "type": "json",
            "pluginOptions": {
                "i18n": {
                    "localized": true
                }
            }
        },
        "relatedBiographies": {
            "type": "relation",
            "relation": "manyToMany",
            "target": "api::biography.biography"
        },
        "featured": {
            "type": "boolean",
            "default": false,
            "pluginOptions": {
                "i18n": {
                    "localized": false
                }
            }
        }
    }
}